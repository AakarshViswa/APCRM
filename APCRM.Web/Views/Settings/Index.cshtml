@model IEnumerable<AppSettings>

<div class="pagetitle">
    <h1>Settings</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li class="breadcrumb-item active">Settings</li>
        </ol>
    </nav>
</div><!-- End Page Title -->

<section class="section">
    <p>
        Use the Following Page to Update the CRM's Data Entities.
    </p>
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">General Settings</h5>
            <div class="iconslist">
                @foreach (var obj in Model)
                {
                    <div class="icon">
                        <a asp-controller="@obj.ControllerName" asp-action="@obj.ViewName">
                            <i class="@obj.Icon"></i>
                            <div class="label">@obj.Name</div>
                        </a>
                    </div>
                }
            </div>
        </div>
    </div>
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">Package Management</h5>
            <div class="iconslist">
                <div class="icon">
                    <a>
                        <i class="ri-product-hunt-fill"></i>
                        <div class="label"> Products </div>
                    </a>
                </div>
                <div class="icon">
                    <a>
                        <i class="ri-shopping-cart-2-fill"></i>
                        <div class="label"> Deliverables </div>
                    </a>
                </div>

                <div class="icon">
                    <a>
                        <i class="bx bxs-package"></i>
                        <div class="label"> Package </div>
                    </a>
                </div>
            </div>
        </div>
    </div>



</section>
